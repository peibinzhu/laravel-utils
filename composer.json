{
    "name": "peibin/laravel-utils",
    "description": "A tools package that could help developer solved the problem quickly.",
    "license": "MIT",
    "keywords": [
        "php",
        "laravel",
        "utils"
    ],
    "authors": [
        {
            "name": "Peibin Zhu",
            "email": "zhupeibin1207@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "laravel/framework": ">=9.0",
        "nikic/php-parser": "^4.1",
        "peibin/laravel-contracts": "^1.0",
        "peibin/laravel-provider-config": "^1.0",
        "php-di/phpdoc-reader": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "PeibinLaravel\\Utils\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PeibinLaravelTest\\Utils\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PeibinLaravel\\Utils\\UtilServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}
